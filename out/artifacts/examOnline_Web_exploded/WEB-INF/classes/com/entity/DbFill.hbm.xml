<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping
            PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.entity.DbFill" table="db_fill" lazy="false">
        <!-- student id -->
        <id name="stID" type="integer">
            <!-- 主键生成策略 -->
            <generator class="increment"></generator>

        </id>
        <property name="stTitle"></property>
        <property name="stAnswer"></property>
        <property name="stParse"></property>
    </class>
</hibernate-mapping>